<p id="notice"><%= notice %></p>

<h1>Parties</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Mobile</th>
      <th>Qbc</th>
      <th>Address</th>
      <th>Refrence 1</th>
      <th>Refrence 2</th>
      <th>Refrence 3</th>
      <th>Gst no</th>
      <th>Pan no</th>
      <th>Adhaar no</th>
      <th>Mobile 2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parties.each do |party| %>
      <tr>
        <td><%= party.name %></td>
        <td><%= party.mobile %></td>
        <td><%= party.qbc %></td>
        <td><%= party.address %></td>
        <td><%= party.refrence_1 %></td>
        <td><%= party.refrence_2 %></td>
        <td><%= party.refrence_3 %></td>
        <td><%= party.gst_no %></td>
        <td><%= party.pan_no %></td>
        <td><%= party.adhaar_no %></td>
        <td><%= party.mobile_2 %></td>
        <td><%= link_to 'Show', party %></td>
        <td><%= link_to 'Edit', edit_party_path(party) %></td>
        <td><%= link_to 'Destroy', party, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Party', new_party_path %>
