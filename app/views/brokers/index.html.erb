<p id="notice"><%= notice %></p>

<h1>Brokers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Mobile</th>
      <th>Qbc</th>
      <th>Address</th>
      <th>Refrence 1</th>
      <th>Refrence 2</th>
      <th>Refrence 3</th>
      <th>Gst no</th>
      <th>Pan no</th>
      <th>Adhaar no</th>
      <th>Mobile 2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @brokers.each do |broker| %>
      <tr>
        <td><%= broker.name %></td>
        <td><%= broker.mobile %></td>
        <td><%= broker.qbc %></td>
        <td><%= broker.address %></td>
        <td><%= broker.refrence_1 %></td>
        <td><%= broker.refrence_2 %></td>
        <td><%= broker.refrence_3 %></td>
        <td><%= broker.gst_no %></td>
        <td><%= broker.pan_no %></td>
        <td><%= broker.adhaar_no %></td>
        <td><%= broker.mobile_2 %></td>
        <td><%= link_to 'Show', broker %></td>
        <td><%= link_to 'Edit', edit_broker_path(broker) %></td>
        <td><%= link_to 'Destroy', broker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Broker', new_broker_path %>
